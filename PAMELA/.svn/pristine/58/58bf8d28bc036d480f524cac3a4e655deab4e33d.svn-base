package lu.uni.fstc.proactivity.rules;

/**
 * Types of Messages importance
 * NOTE: not an ENUM, to save on the database as a Numerical attribute of the message
 * @author Sandro Reis
 * @version 3.0 - Sandro Reis © 2012 
 *
 */
public class Importance {

	/**
	 * GLOBAL Constant to represent a VERY LOW importance
	 */
	public static final int VERY_LOW = -100;
	/**
	 * GLOBAL Constant to represent a LOW importance
	 */
	public static final int LOW = -50;
	/**
	 * GLOBAL Constant to represent a NORMAL importance
	 */
	public static final int NORMAL = 0;
	/**
	 * GLOBAL Constant to represent a HIGH importance
	 */
	public static final int HIGH = 50;
	/**
	 * GLOBAL Constant to represent a VERY HIGH importance
	 */
	public static final int VERY_HIGH = 100;
	/**
	 * GLOBAL Constant to represent a EXTREMELY HIGH importance
	 */
	public static final int EXTREMELY_HIGH = 200;
}
