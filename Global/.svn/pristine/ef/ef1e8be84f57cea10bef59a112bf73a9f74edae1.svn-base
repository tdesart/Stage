package lu.uni.fstc.proactivity.queue;

/**
 * <b>Represents one node from the Queue</b><br>
 * has the following attributes:<br>
 * &nbsp;&nbsp;&nbsp;• one (generic) Item<br>
 * &nbsp;&nbsp;&nbsp;• one pointer/refence to the next Node
 *  
 * @author Sandro Reis
 * @version 3.0 - Sandro Reis © 2011 
 * @version 2.0 - Sergio Marques Dias <br> 
 * @version 1.0 - Yann Milin
 * @param <Item> Generic
 * 
 */
public class Node<Item> {

	private Item item;
	private Node<Item> next;

	/**
	 * @return item - a generic Item
	 */
	public Item getItem() {
		return item;
	}

	/**
	 * @param item - a generic Item
	 */
	public void setItem(Item item) {
		this.item = item;
	}

	/**
	 * @return next - a Node
	 */
	public Node<Item> getNext() {
		return next;
	}

	/**
	 * @param next - a Node
	 */
	public void setNext(Node<Item> next) {
		this.next = next;
	}
}
